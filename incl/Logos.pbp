'-------------------------------------------------------------------------------
Wheel:
    PosX=0:PosY=0:Gosub Lcd_GotoXY
    TEMP = 0
    gosub Lcd_Clear
    for temp2 = 0 to 5
        for temp1 = 0 to 2
            FOR k = 0 to 5
                PosX=TEMP:PosY=(k+A):Gosub Lcd_GotoXY    'Wheel Logos
                for i = 0 to 7
                    I2CAddressEEprom = (2924+(i*6)+(k*48)+(temp1*288)):gosub Lue_I2C
                next i
            next k
            temp = temp + 2
        next temp1
    next temp2
    temp1 = 0
    temp2 = 0
    temp = 0
    return
'-------------------------------------------------------------------------------
Stop_Logo:
    FOR k = 0 to 5
        PosX=18:PosY=(k+A):Gosub Lcd_GotoXY     'Stop Logo
        for i = 0 to 7
            I2CAddressEEprom = (1304 + (i*6) + (k*48)):gosub Lue_I2C
        next i
    next k
    return
'-------------------------------------------------------------------------------
Kawa_Logo:
    FOR k = 0 to 5
        PosX=0:PosY=(k+A):Gosub Lcd_GotoXY     'Kawa Logo
        for i = 0 to 13
            I2CAddressEEprom = ((i*6) + (k*84)):gosub Lue_I2C
        next i
